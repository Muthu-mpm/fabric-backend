<div class="modal fade" id="addbranchmodal" tabindex="-1" role="dialog" aria-labelledby="addbranchmodal" aria-hidden="true">
  <form id="subscribe-email-form"class="form-horizontal" role="form" action="/adduser/" method="post">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalCenterTitle">Add User</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="form-group">
            <div class="row">
              <div class="col-3 offset-1">  <label class="control-label" for="name">Name</label>  </div>
              <div class="col-8">  <input type="text" placeholder="Name" class="form-control" name="name"> </div>
            </div>
          </div>

      <div class="form-group">
        <div class="row">
          <div class="col-3 offset-1">  <label class="control-label" for="apptype">App Type</label>  </div>
          <div class="col-8"> <select class="form-control" id="sel1" name="apptype">
    <option>Oyna Beam</option>
    <option>Oyna Billboard</option>
    <option>Oyna Botique</option>
    <option>Oyna Manager</option>
  </select> </div>
        </div>
      </div>

      <!-- Text input-->
      <div class="form-group">
        <div class="row">
          <div class="col-3 offset-1">  <label class="control-label" for="username">User name</label>  </div>
          <div class="col-8">  <input type="email" placeholder="sample@email.com" class="form-control" name="username"> </div>
        </div>
      </div>

      <!-- Text input-->
      <div class="form-group">
        <div class="row">
          <div class="col-3 offset-1">  <label class="control-label" for="password">Password</label>  </div>
          <div class="col-8">  <input type="password" placeholder="City" class="form-control"name="password"> </div>
        </div>
      </div>

        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
          <button type="submit" class="btn btn-primary">Add User</button>
        </div>
      </div>
    </div>
     <input type="hidden" id="branch" name="branchid" value="<%=branchid%>">
  </form>
</div>
