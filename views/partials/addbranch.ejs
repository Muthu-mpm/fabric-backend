<div class="modal fade" id="addbranchmodal" tabindex="-1" role="dialog" aria-labelledby="addbranchmodal" aria-hidden="true">
  <form id="subscribe-email-form"class="form-horizontal" role="form" action="/addbranch/" method="post">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalCenterTitle">Add Branch</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="form-group">
            <div class="row">
              <div class="col-3 offset-1">  <label class="control-label" for="name">Name</label>  </div>
              <div class="col-8">  <input type="text" placeholder="Branch Name" class="form-control" name="name"> </div>
            </div>
          </div>

      <div class="form-group">
        <div class="row">
          <div class="col-3 offset-1">  <label class="control-label" for="addressline1">Address Line 1</label>  </div>
          <div class="col-8"><input type="text" placeholder="Address Line 1" class="form-control"name="addressline1"> </div>
        </div>
      </div>

      <!-- Text input-->
      <div class="form-group">
        <div class="row">
          <div class="col-3 offset-1">  <label class="control-label" for="addressline2">Address Line 2</label>  </div>
          <div class="col-8">  <input type="text" placeholder="Address Line 2" class="form-control" name="addressline2"> </div>
        </div>
      </div>

      <!-- Text input-->
      <div class="form-group">
        <div class="row">
          <div class="col-3 offset-1">  <label class="control-label" for="city">City</label>  </div>
          <div class="col-8">  <input type="text" placeholder="City" class="form-control"name="city"> </div>
        </div>
      </div>

      <!-- Text input-->
      <div class="form-group">
        <div class="row">
          <div class="col-3 offset-1">  <label class="control-label" for="state">State</label>  </div>
          <div class="col-3">     <input type="text" placeholder="State" class="form-control" name="state"> </div>
            <div class="col-2">  <label class="control-label" for="postcode">Postcode</label>  </div>
          <div class="col-3">    <input type="text" placeholder="Post Code" class="form-control" name="postcode"></div>
        </div>
      </div>

      <div class="form-group">
        <div class="row">
          <div class="col-3 offset-1">  <label class="control-label" for="country">Country</label>  </div>
          <div class="col-8">  <input type="text" placeholder="country" class="form-control " name="country"> </div>
        </div>
      </div>

        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
          <button type="submit" class="btn btn-primary">Add Branch</button>
        </div>
      </div>
    </div>
  </form>
</div>
