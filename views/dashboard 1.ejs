
<%- include('partials/header') %>

  <h1>Welcome to Oyna</h1>
  <div class=""style="height:10px;">
  </div>
  <form action="/uploadfile" method="post" enctype="multipart/form-data">
  <input type="file" name="oynafile" />
    <button type="submit" class="btn btn-dark">Upload</button>
</form>
<div class=""style="height:100px;">
</div>
<div class="table">
  <table  class="table">
    <thead>
          <th>#</th>
    <th>File Name </th>
  <th>File Path </th>
    <th>Store </th>
      <th>Consumers </th>
    </th>
  </thead>
  <% for(var i=0; i < people.length; i++) { %>
     <tr>
        <td><%= i %></td>
       <td><a href=<%= "/download_File/"+people[i].fileid %>><%= people[i].filename %></a></td>
       <td><%= people[i].filepath %></td>
        <td><%= people[i].store %></td>
         <td>
           <%for(var j=0;j< people[i].consumers.length;j++){
             if(j!=0){ %>
             <%=","%>
             <% } %>
             <%= people[i].consumers[j].user %>

          <% } %>

         </td>
     </tr>
  <% } %>
  </table>
</div>

        <script>
          var useracc= '<%- JSON.stringify(people)%>' ;
  console.log(useracc);
</script>
<%- include('partials/footer') %>
